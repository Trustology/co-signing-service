{
  "name": "@trustology/co-signing-service",
  "version": "0.1.0",
  "description": "The co-signing-service is a service designed to co-sign TrustVault transactions under a given set of rules.",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "package": "npm run clean && npx tsc && cp package.json ./build && cp package-lock.json ./build",
    "clean": "rm -rf ./build ./dist",
    "audit": "npm audit",
    "start": "ts-node client-examples/example-express-app.ts",
    "style-check": "prettier --check src/**/*.ts *.json",
    "style": "prettier --write src/**/*.ts *.json",
    "quality-check": "tslint --project . --format stylish",
    "quality": "tslint --project . --format stylish --fix",
    "test": "nyc mocha -r ts-node/register './client-examples/**/*.spec.ts' --timeout 5000",
    "int-test": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/Trustology/co-signing-service.git"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/body-parser": "1.19.0",
    "@types/chai": "4.2.12",
    "@types/express": "4.17.8",
    "@types/mocha": "8.0.3",
    "@types/node": "14.10.0",
    "@types/supertest": "2.0.10",
    "@types/bignumber.js": "5.0.0",
    "body-parser": "1.19.0",
    "chai": "4.2.0",
    "express": "4.17.1",
    "mocha": "8.1.3",
    "nyc": "15.1.0",
    "prettier": "2.1.1",
    "tslint": "6.1.3",
    "supertest": "4.0.2",
    "tslint-config-prettier": "1.18.0"
  },
  "dependencies": {
    "bignumber.js": "9.0.0",
    "ts-node": "9.0.0",
    "@trustology/trustvault-nodejs-sdk": "1.0.0",
    "typescript": "4.0.2"
  }
}
